<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#97cf8f" />
            <gradient android:angle="-90" android:startColor="#538c59" android:endColor="#86c48f"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#97cf8f" />
            <solid android:color="#739665"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#97cf8f" />
            <gradient android:angle="-90" android:startColor="#91c799" android:endColor="#739665" />
        </shape>
    </item>
</selector>